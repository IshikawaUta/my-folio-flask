{
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "requirements": "requirements.txt"
        }
      }
    ],
    "routes": [
    {
      "src": "/robots.txt",
      "dest": "app.py"
    },
    {
      "src": "/sitemap.xml",
      "dest": "app.py"
    },
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
